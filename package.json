{
  "name": "excel-templator",
  "version": "0.3.1",
  "description": "Excel templator",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "excel",
    "elsx",
    "template"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "npm-version": "npm version patch",
    "npm-publish": "npm publish",
    "release": "npm-run-all clean compile npm-version npm-publish",
    "test": "jest --runInBand",
    "web-clean": "rimraf ./dist",
    "web-compile": "webpack",
    "web-build": "npm-run-all web-clean web-compile",
    "web-demo-compile": "webpack --mode development",
    "web-demo-build": "npm-run-all web-clean web-compile",
    "server": "http-server",
    "demo": "npm-run-all web-demo-build server"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.spec.+(ts|tsx|js)"
    ],
    "preset": "ts-jest"
  },
  "dependencies": {
    "base64-arraybuffer": "^1.0.1",
    "buffer": "^6.0.3",
    "exceljs": "^4.3.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.9",
    "http-server": "^14.1.0",
    "image-size": "^1.0.1",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "author": "Yoshihiro Seki",
  "license": "MIT"
}
